{
    "name": "alva/array-eloquent-driver",
    "description": "Eloquent driver for array data",
    "keywords": [
        "alva",
        "laravel",
        "array-eloquent-driver"
    ],
    "homepage": "https://alvacommerce.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii",
            "email": "a.korposh@alvacommerce.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": ">=11.0.0",
        "spatie/laravel-package-tools": "^1.13.5"
    },
    "require-dev": {
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Alva\\ArrayEloquentDriver\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alva\\ArrayEloquentDriver\\ArrayEloquentDriverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
